#include "holberton.h"

/**
 * print_rev - prints a string, in reverse, followed by a new line
 *
 * @s: string
 *
 *Returns: string in reverse
 */

void print_rev(char *s)


{
	int c;

	for (c = 0; s[c] != '\0'; c++)
		;

	i = cint _strlen(char *s)
		{
			int c;

			for (c = 0; s[c] != '\0'; c++)
				;

			return (c);
		};


	int i;

	for(i = _strlen(*s); s[i] != '\0'; i--)

	{
		_putchar(s[i]);
	}
	_putchar('\n');
}
